#!/usr/bin/env groovy
pipeline {
    
    agent {
        docker {
            image 'maven'
        }
    }
    stages {
        stage('Build') {
            steps {
                sh "mvn -version"
                sh "mvn clean install"
            }
        }
    }

    post {
        always {
            cleanWs()
        }
    }
}
